{
  "name": "bilibili-video-parser",
  "version": "1.0.0",
  "description": "Bilibili video link parser - converts various Bilibili URL formats to playable stream URLs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "typecheck": "tsc --noEmit",
    "dev": "tsx apps/server/index.ts"
  },
  "keywords": [
    "bilibili",
    "video",
    "parser",
    "stream",
    "dash",
    "flv"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^20.10.0",
    "fast-check": "^4.5.3",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "punycode": "^2.3.1"
  }
}
