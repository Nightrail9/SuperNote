# Server
PORT=3000
CORS_ORIGIN=*

# Bilibili (optional)
SESSDATA=

# Model settings are stored in data/app-settings.json

# Web mode: Jina Reader
JINA_READER_ENDPOINT=https://r.jina.ai/
JINA_API_KEY=
JINA_READER_TIMEOUT_SEC=30
JINA_READER_NO_CACHE=false

# Local transcription (Bilibili mode) using faster-whisper
# Python interpreter path (relative to project root or absolute path)
# Multi-link task concurrency (1-4)
NOTE_LINK_CONCURRENCY=2
LOCAL_ASR_COMMAND=env/python.exe
LOCAL_ASR_MODEL=small
LOCAL_ASR_LANGUAGE=zh
LOCAL_ASR_DEVICE=auto
LOCAL_ASR_BEAM_SIZE=5
LOCAL_ASR_TEMPERATURE=0
LOCAL_ASR_TIMEOUT_MS=1800000
